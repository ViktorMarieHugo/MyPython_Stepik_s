a = 13  # по умолчанию, если прописать число, то оно int
b = 7  # при вводе чила с клавиатуры используется формат int(input())
# для преобразования числа применяем оператор int()
total = a + b
diff = a - b
prod = a * b
div1 = a / b
div2 = a // b
mod = a % b
exp = a**b

print(a, "+", b, "=", total)
print(a, "-", b, "=", diff)
print(a, "*", b, "=", prod)
print(a, "/", b, "=", div1)
print(a, "//", b, "=", div2)
print(a, "%", b, "=", mod)
print(a, "**", b, "=", exp)
