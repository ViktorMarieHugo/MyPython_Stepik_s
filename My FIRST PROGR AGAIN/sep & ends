a = input("enter your day ")
b = input("enter your month ")
c = input("enter your year ")
print()
print(a, b, c, sep="***")
print()
print(a, b, c, end="@@@")
print()
print(a, b, c, end="###")
print()
print(a, b, c, sep="-", end="\n\n\n")


print("a", "b", "c")
